<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="clustere.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="CDEVA perspective"
            class="clustere.Perspective"
            id="CDEVA.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="clustere.views.ViewPart1"
            id="CDEVA.view1"
            name="Cluster Results"
            restorable="true">
      </view>
      <view
            class="clustere.views.ViewPart2"
            id="CDEVA.view2"
            name="Property"
            restorable="true">
      </view>
      <view
            class="clustere.views.CurrentNetworkView"
            id="CDEVA.view3"
            name="CurrentNetwork"
            restorable="true">
      </view>
      <view
            class="clustere.views.AlgorithmInfoView"
            id="CDEVA.view4"
            name="Using Algorithms"
            restorable="true">
      </view>

      <!--
      <view
            class="clustere.views.PredictionView"
            id="CDEVA.view5"
            name="Gene Table"
            restorable="true">
      </view>
      -->
      <view
            class="clustere.views.EdgeView"
            id="CDEVA.EdgeView"
            name="Edges Table"
            restorable="true">
      </view>

   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="clustere.editors.ClusterEditor"
            default="false"
            id="CDEVA.editor1"
            name="Clusters View">
      </editor>
      <editor
            class="clustere.editors.EvaluationView"
            default="false"
            id="CDEVA.editor2"
            name="Evaluation Result View">
      </editor>
      <editor
            class="clustere.editors.NetworkView"
            default="true"
            id="CDEVA.editor3"
            name="Network View">
      </editor>

<!--
<editor
            class="clustere.editors.PredictionEditor"
            default="false"
            id="CDEVA.editor4"
            name="Prediction View">
      </editor>
-->
      

      
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="CDEVA.application"
            name="CDEVA">
         <property
               name="appName"
               value="CDEVA">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="7,310,445,20">
         </property>
         <property
               name="startupProgressRect"
               value="0,330,500,18"
               >
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="aboutImage"
               value="icons/alt_about.gif">
         </property>
         <property
               name="aboutText"
               value="C-DEVA (An analysis platform for Clusters of biological networks Detection, Evaluation, Visualization and Annotation)&#x0A;is designed by YuTang and Xuehong Wu from Central Souoth University.  &#x0A;If you any problems or suggestions of C-DEVA, please contact us at Email: tangyu333@gmail.com.">
         </property>
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif,icons/alt_window_48.gif,icons/alt_window_64.gif,icons/alt_window_128.gif">
         </property>
      </product>
   </extension>

</plugin>
